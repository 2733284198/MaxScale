[maxscale]
threads=###threads###
users_refresh_time=0

[rwsplit-service]
type=service
router=readwritesplit
weightby=weight
user=maxskysql
password=skysql

[read-connection-router-slave]
type=service
router=readconnroute
user=maxskysql
password=skysql

[read-connection-router-master]
type=service
router=readconnroute
router_options=master
user=maxskysql
password=skysql

[CLI]
type=service
router=cli

[CLI-Listener]
type=listener
service=CLI
protocol=maxscaled
socket=default
